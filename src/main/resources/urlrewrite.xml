<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN" 
"http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<urlrewrite>

	
	<rule>
		<from>admin/</from>
		<to>/Backstage/Sysmanager/Wfstaff/login.jsp</to>
	</rule>
	<rule>
		<from>/weixin/</from>
		<to>/wechat/wechatFilter/doFilter.jhtml</to>
	</rule>
	<rule>
		<from>/wx/([0-9]+)/(.*).jhtml</from>  
	    <to>/$2.jhtml?accountId=$1</to> 
	</rule>
	<rule>
		<from>/detail_cityCode_([0-9]+).htm</from>
	    <to>/lvxbang/destination/detail.jhtml?cityCode=$1</to>
	</rule>
</urlrewrite>